{
  "restApiRoot": "/cassia",
  "host": "0.0.0.0",
  "port": 3030,
  "remoting": {
    "context": false,
    "rest": {
      "handleErrors": false,
      "normalizeHttpPath": false,
      "xml": false
    },
    "json": {
      "strict": false,
      "limit": "100kb"
    },
    "urlencoded": {
      "extended": true,
      "limit": "100kb"
    },
    "cors": false
  },
  "httpMode": false,
  "certConfig": {
    "path": "crt/",
    "key": "key.pem",
    "cert": "certificate.crt",
    "ca": [],
    "requestCert": false,
    "rejectUnauthorized": false
  }
}
